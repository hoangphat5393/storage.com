{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ckfinder/ckfinder-laravel-package": "^5",
        "gornymedia/laravel-shortcodes": "^1.3",
        "guzzlehttp/guzzle": "^7.9",
        "intervention/image-laravel": "^1.5",
        "josiasmontag/laravel-recaptchav3": "^1.0",
        "laravel/framework": "^12",
        "laravel/sanctum": "^4.2",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.10",
        "laravel/ui": "^4.6",
        "maatwebsite/excel": "^3.1",
        "symfony/http-client": "^7.3",
        "symfony/mailgun-mailer": "^7.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "fakerphp/faker": "^1.24",
        "kitloong/laravel-migrations-generator": "^7.1",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.44",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Libraries/system.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "codezero/composer-preload-files": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
